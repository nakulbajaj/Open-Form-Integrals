   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"x", "[", "k", "]"}], ",", 
       RowBox[{"y", "[", "k", "]"}]}], "}"}], "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Random", "[", 
        RowBox[{"Real", ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", 
           RowBox[{
            SqrtBox["\[Pi]"], "/", "2"}]}], "}"}], ",", "3"}], "]"}], ",", 
       RowBox[{"Random", "[", 
        RowBox[{"Real", ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", "1"}], "}"}], ",", "3"}], "]"}]}], "}"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"k", ",", "1", ",", "darts"}], "}"}]}], "]"}], " "}]], "Input",
